<div class="posts">
    {{#each posts as |post|}}
    <div class="container">
        <div class="card mt-2">
            <div class="card-header" id={{post.id}}>{{post.title}} <span class="date">{{format_date post.date_created}}</span></div>
        </div>
    </div>
    {{/each}}
</div>

{{#if post}}
<script src="./js/dashboard.js"></script>
{{/if}}